generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  email            String?  @unique
  favorites        String[]
  gym              String?
  id               Int      @default(autoincrement()) @id
  imgurl           String?
  name             String?
  password         String?
  resetToken       String? @unique @map (name: "resettoken")
  resetTokenExpiry String?         @map (name: "resettokenexpiry")

  @@map (name: "users")
}
